{
  "name": "weather-101",
  "version": "1.0.0",
  "description": "React application displaying weather data",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack -p --config ./webpack.prod.config.js --progress --colors",
    "start": "webpack-dev-server --config webpack.config.js",
    "test": "jest ./__tests__/"
  },
  "author": "James Robinson <jrobind@outlook.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-jest": "23.0.1",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "css-loader": "0.28.11",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.1.0",
    "node-sass": "^7.0.0",
    "sass-loader": "7.0.1",
    "style-loader": "0.21.0",
    "url-loader": "1.0.1",
    "webpack": "4.8.3",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "2.1.3",
    "webpack-dev-server": "^3.7.1"
  },
  "dependencies": {
    "moment": "2.22.2",
    "npm": "^6.9.0",
    "prop-types": "15.6.1",
    "rc-progress": "2.2.5",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-moment": "0.7.0",
    "react-places-autocomplete": "7.1.2",
    "react-router-dom": "4.2.2"
  },
  "jest": {
    "collectCoverage": true,
    "setupTestFrameworkScriptFile": "<rootDir>/test-setup.js",
    "moduleNameMapper": {
      "^.+\\.(css|less|scss)$": "identity-obj-proxy",
      "\\.(svg|png|jpg)$": "<rootDir>/testHelpers/empty-module.js"
    }
  }
